<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.Tarea2_2_Aplicacion_ListView_api">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RetrofitApp">
        <activity
            android:name="com.example.Tarea2_2_Aplicacion_ListView_api.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.Tarea2_2_Aplicacion_ListView_api.ListaActivity"
            android:exported="true">
        </activity>
        <activity
            android:name="com.example.Tarea2_2_Aplicacion_ListView_api.BusquedaActivity"
            android:exported="true"></activity>
    </application>

</manifest>